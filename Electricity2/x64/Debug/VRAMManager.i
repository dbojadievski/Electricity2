VRAMPool*
VRAMManager::FindPrevVRAMPoolInChain( VRAMPool* pPool ) noexcept
{
	VRAMPool* pPrevPool		= ( s_pVRAMPools != pPool ? s_pVRAMPools : nullptr );

	if ( pPrevPool )
	{
		while ( pPrevPool )
